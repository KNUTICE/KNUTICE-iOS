# Uncomment the next line to define a global platform for your project
platform :ios, '15.0'

target 'KNUTICE' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Alamofire
  pod 'Alamofire'

  # ReactiveX
  pod 'RxSwift', '6.5.0'
  pod 'RxCocoa', '6.5.0'
  pod 'RxDataSources'

  # SnapKit
  pod 'SnapKit'

  # Kingfisher
  pod 'Kingfisher', '~> 7.0'

  # SkeletonView
  pod 'SkeletonView'

  # Google Firebase
  pod 'FirebaseAnalytics'
  pod 'FirebaseMessaging'
end
